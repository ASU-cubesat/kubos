{
    "name" : "msp430f5529-gcc",
    "version" : "0.0.4",
    "inherits": {
      "kubos-msp430-gcc": "openkosmosorg/target-kubos-msp430-gcc#~0.0.1"
    },
    "description" : "Official kubos-rt base target description for compiling with the msp430-gcc compiler against the msp430f5529 mcu",
    "license" : "Apache-2.0",
    "similarTo" : [
        "msp430",
        "msp430-gcc",
        "gcc",
        "freertos",
        "kubos-rt",
        "msp430f5529",
        "msp430f5529-gcc"
    ],
    "toolchain" : "CMake/toolchain.cmake",
    "config": {
        "hardware": {
	        "i2cCount": 2,
            "spiCount": 2,
            "defaults": {
                "i2c": "K_I2C2",
                "spi": "K_SPI1"
            },
            "console": {
                "uart": "K_UART2",
                "baudRate": 115200
            },
            "uart": {
                "count": 2,
                "uart1": {
                    "tx": "P33",
                    "rx": "P34"
                },
                "uart2": {
                    "tx": "P44",
                    "rx": "P45"
                }
            }
        }
    }
}
