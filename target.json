{
  "name": "stm32f407-disco-gcc",
  "version": "0.0.3",
  "inherits": {
    "kubos-arm-none-eabi-gcc": "openkosmosorg/target-kubos-arm-none-eabi-gcc#master"
  },
  "description": "KubOS build target for the stm32f407-discovery development board.",
  "author": "Marshall Culpepper <marshall@kubos.co>",
  "homepage": "https://github.com/openkosmosorg/target-stm32f407-disco-gcc",
  "licenses": [
    {
      "url": "https://spdx.org/licenses/Apache-2.0",
      "type": "Apache-2.0"
    }
  ],
  "keywords": [
    "stm32f407-disco",
    "gcc"
  ],
  "similarTo": [
    "stm32f407-disco",
    "f407vg",
    "f407",
    "discovery",
    "stm32f407vgt6",
    "stm32f407vg",
    "stm32f407xg",
    "stm32f407",
    "stm32f4",
    "stm32",
    "st",
    "cortex-m4",
    "armv7-m"
  ],
  "config": {
    "cmsis": {
      "nvic": {
        "ram_vector_address": "0x20000000",
        "flash_vector_address": "0x08000000",
        "user_irq_offset": 16,
        "user_irq_number": 82
      }
    },
    "uvisor": {
      "present": 0
    },
    "hardware": {
      "externalClock": "8000000",
      "pins": {
        "LED1": "PD_12",
        "LED2": "PD_13",
        "LED3": "PD_14",
        "LED4": "PD_15",
        "USER_BUTTON": "PA_0",
        "SERIAL_TX": "PA_2",
        "SERIAL_RX": "PA_3",
        "USBTX": "PA_2",
        "USBRX": "PA_3",
        "I2C_SCL": "PB_8",
        "I2C_SDA": "PB_9",
        "SPI_MOSI": "PA_7",
        "SPI_MISO": "PA_6",
        "SPI_SCK": "PA_5",
        "SPI_CS": "PA_4",
        "PWM_OUT": "PB_3"
      },
      "test-pins": {
        "spi": {
          "mosi": "SPI_MOSI",
          "miso": "SPI_MISO",
          "sclk": "SPI_SCK",
          "ssel": "SPI_CS"
        },
        "i2c": {
          "sda": "I2C_SDA",
          "scl": "I2C_SCL"
        },
        "serial": {
          "tx": "PC_12",
          "rx": "PF_6"
        }
      }
    }
  },
  "toolchain": "CMake/toolchain.cmake",
  "debug": [
    "valinor",
    "--target",
    "stm32f407vg",
    "$program"
  ],
  "scripts": {
    "debug": [
      "valinor",
      "--target",
      "stm32f407vg",
      "$program"
    ],
    "test": [
      "mbed_test_wrapper",
      "--target",
      "STM32F4-DISCO",
      "$program"
    ],
    "start": [
      "bash",
      "$PWD/yotta_targets/stm32f407-disco-gcc/openocd/flash.sh",
      "$PWD/yotta_targets/stm32f407-disco-gcc/openocd/",
      "stm32f4_flash $program"
    ]
  }
}
